marketo.syncInterests.sql=select top 50 a.email, c.name, c.code FROM contacts a \
    LEFT JOIN ContactsInterests b ON a.id = b.contactid  \
    LEFT JOIN interests c ON b.interestid = c.id \
    INNER JOIN Contacts d on b.contactid = d.id WHERE a.email IN (select key1 FROM MarketoStatus where objectType = 'Lead' and status IN ('CREATED', 'UPDATED'))  \
    and a.email not in (select key1 from MarketoStatus where objectType = 'Interest' and status in ('CREATED', 'UPDATED')) order by email
marketo.syncInterests.sqlBefore=\
  update contacts set luInterests = '1970-01-01' where email in (\
  select top 50 a.email FROM contacts a \
  LEFT JOIN ContactsInterests b ON a.id = b.contactid  \
  LEFT JOIN interests c ON b.interestid = c.id \
  INNER JOIN Contacts d on b.contactid = d.id WHERE a.email IN (select key1 FROM MarketoStatus where objectType = 'Lead' and status IN ('CREATED', 'UPDATED'))  \
  and a.email not in (select key1 from MarketoStatus where objectType = 'Interest' and status in ('CREATED', 'UPDATED')) order by email \
  )
marketo.syncInterests.sqlAfterSuccess=\
  update contacts set luInterests = GETDATE() where lastUploaded = '1970-01-01'
marketo.syncInterests.sqlAfterFailure=\
  update contacts set luInterests = null where luInterests = '1970-01-01'
marketo.syncInterests.marketoKeys=EmailAddress
marketo.syncInterests.sqlWorkerBeforeSingle=\
  update contacts set worker = ? where id = ?
marketo.syncInterests.sqlWorker=select a.email, c.name, c.code FROM contacts a \
    LEFT JOIN ContactsInterests b ON a.id = b.contactid  \
    LEFT JOIN interests c ON b.interestid = c.id \
    WHERE a.worker = ? order by email
marketo.syncInterests.sqlWorkerAfterSuccess=\
  update contacts set luInterests = GETDATE(), worker = null where worker = ?
marketo.syncInterests.sqlWorkerAfterFailure=\
  update contacts set luInterests = null, worker = null where worker = ?

marketo.syncInterests.sqlInsertStatus=\
  insert into MarketoStatus (objectType, status, error, key1, lastUpdated) values (?, ?, ?, ?, getdate())

