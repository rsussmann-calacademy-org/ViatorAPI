        <MessageID>$messageId</MessageID>
        <MessageType>AbandonSession</MessageType>
        <SessionID>$sessionId</SessionID>
        <TimeStamp>$timestamp</TimeStamp>
    </Header>
    <Body>
    <AbandonSession>
        <SessionID>$sessionId</SessionID>
    </AbandonSession>
    </Body>
</Envelope>